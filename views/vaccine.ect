<!DOCTYPE html>

<html>
<head>
	<link rel="stylesheet" href="../bootstrap/dist/css/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="../jquery-ui/themes/smoothness/jquery-ui.min.css">

	<script type="text/javascript" src="../jquery/dist/jquery.min.js"></script>
	<script type="text/javascript" src="../bootstrap/dist/js/bootstrap.min.js"></script>
	<script type="text/javascript" src="../jquery-ui/jquery-ui.min.js"></script>
	 <script>
  $(function() {
    $( "#datepicker" ).datepicker();
  });
  </script>

</head>
<body>
	<nav class="navbar navbar-inverse">
		<ul class="nav navbar-nav">
		<li class="active"><a href="/parent">Home</a></li>
		<li class="dropdown"><a class="dropdown-toggle" data-toggle="dropdown" href="#">Vaccine <span class="caret"></span></a>

			<ul class = "dropdown-menu">
				<% if @illnessList?.length : %>
					<% for illness in @illnessList : %>
					<li><a href="<%- illness._id %>"> <%- illness.type %> </a></li>

					<% end %>
				<% end %>
			</ul>
		</li>
		<li><a href="#">Page 2</a></li>
		<li><a href="#">Page 3</a></li>
	</ul>
	</nav>
	<div class="vaccine_list">
		<h4><%- @illness %></h4><br><br>
		<% if @vaccineList.length : %>
		<table class ="table">
			<tr>
				<th>Tên vaccine</th>
				<th>Xuất xứ</th>
				<th>Phòng bệnh</th>
			</tr>
			<% for vaccine in @vaccineList : %>
			<tr>
				<td><%- vaccine.vaccine_name %></td>
				<td><%- vaccine.vaccine_country %></td>
				<td><%- vaccine.vaccine_illness %></td>
			</tr>
			<% end %>
		</table>
		<% end %>

	</div>
</body>
</html>
